
<div class="container">
  <div class="p-2">
    <h1>My Imports</h1>
  </div>
  <div class="shadow-lg p-3 mb-5 bg-body rounded">
    <table class="table">
      <thead>
        <tr>
          <th>Code</th>
          <th>Status</th>
          <th>Created</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @imports.each do |import| %>
          <tr>
            <td><%= import.code %></td>
            <td><%= color_status(import.status) %></td>
            <td><%= import.created_at %></td>
            <td>
              <%= link_to import_import_lines_path(import) do %>
                <span><i class="far fa-eye"></i></span>
              <% end %>
            </td>
          </tr>  
        <% end %>
      </tbody>
    </table>
  </div>
  <%= pagy_bootstrap_nav(@pagy).html_safe %>
</div>